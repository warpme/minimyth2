diff --speed-large-files --no-dereference --minimal -Naur linux-6.12.10/arch/arm64/boot/dts/allwinner/sun55i-a527-radxa-a5e.dts linux-6.12.10/arch/arm64/boot/dts/allwinner/sun55i-a527-radxa-a5e.dts
