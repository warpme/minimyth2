diff --speed-large-files --no-dereference --minimal -Naur linux-6.19-rc8/arch/arm64/boot/dts/rockchip/rk3588s-rock-5a.dts linux-6.19-rc8/arch/arm64/boot/dts/rockchip/rk3588s-rock-5a.dts
