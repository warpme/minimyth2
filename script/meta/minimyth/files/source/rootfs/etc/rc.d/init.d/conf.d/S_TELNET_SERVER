################################################################################
# MM_TELNET_SERVER configuration variables and files.
################################################################################
conf_write_S_TELNET_SERVER() {
    local conf_file=$1

    if [ -z "${MM_TELNET_SERVER_ENABLED}" ] ; then
        MM_TELNET_SERVER_ENABLED='yes'
    fi

    conf_variable_write "${conf_file}" MM_TELNET_SERVER_ENABLED "${MM_TELNET_SERVER_ENABLED}"

    return 0
}
