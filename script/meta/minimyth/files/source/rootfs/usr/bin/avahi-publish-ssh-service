#!/bin/sh

. /etc/rc.d/functions

/usr/bin/avahi-publish -s "${MM_HW_DEVICE_NAME}: SSH" _ssh._tcp 22 "/"

#<?xml version="1.0" standalone='no'?><!--*-nxml-*-->
