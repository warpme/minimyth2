diff --speed-large-files --no-dereference --minimal -Naur linux-6.1.9/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts linux-6.1.9/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts
--- linux-6.1.9/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts	1970-01-01 01:00:00.000000000 +0100
+++ linux-6.1.9/arch/arm64/boot/dts/rockchip/rk3588-rock-5b.dts	2023-02-03 15:39:16.037769685 +0100
