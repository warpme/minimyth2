diff --speed-large-files --no-dereference --minimal -Naur linux-6.19/arch/arm64/boot/dts/allwinner/sun55i-a527-cubie-a5e.dts linux-6.19/arch/arm64/boot/dts/allwinner/sun55i-a527-cubie-a5e.dts
--- linux-6.19/arch/arm64/boot/dts/allwinner/sun55i-a527-cubie-a5e.dts	2026-02-08 22:03:27.000000000 +0100
