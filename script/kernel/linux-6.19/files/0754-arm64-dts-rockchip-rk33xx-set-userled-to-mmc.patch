diff --speed-large-files --no-dereference --minimal -Naur linux-6.19-rc8/arch/arm64/boot/dts/rockchip/rk3399-rock-4c-plus.dts linux-6.19-rc8/arch/arm64/boot/dts/rockchip/rk3399-rock-4c-plus.dts
--- linux-6.19-rc8/arch/arm64/boot/dts/rockchip/rk3399-rock-4c-plus.dts	2026-02-01 23:01:13.000000000 +0100
+++ linux-6.19-rc8/arch/arm64/boot/dts/rockchip/rk3399-rock-4c-plus.dts	2026-02-03 13:01:37.071804496 +0100
@@ -15,8 +15,8 @@
 
 	aliases {
 		ethernet0 = &gmac;
-		mmc0 = &sdhci;
-		mmc1 = &sdmmc;
+		mmc0 = &sdmmc;
+		mmc1 = &sdhci;
 	};
 
 	chosen {
@@ -37,18 +37,19 @@
 
 		/* USER_LED1 */
 		led-0 {
-			function = LED_FUNCTION_POWER;
+			function = LED_FUNCTION_STATUS;
 			color = <LED_COLOR_ID_GREEN>;
 			default-state = "on";
 			gpios = <&gpio3 RK_PD4 GPIO_ACTIVE_LOW>;
+			linux,default-trigger = "heartbeat";
 		};
 
 		/* USER_LED2 */
 		led-1 {
-			function = LED_FUNCTION_STATUS;
+			function = LED_FUNCTION_DISK_ACTIVITY;
 			color = <LED_COLOR_ID_BLUE>;
 			gpios = <&gpio3 RK_PD5 GPIO_ACTIVE_HIGH>;
-			linux,default-trigger = "heartbeat";
+			linux,default-trigger = "disk-activity";
 		};
 	};
 
