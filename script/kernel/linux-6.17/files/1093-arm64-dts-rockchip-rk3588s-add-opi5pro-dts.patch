diff --speed-large-files --no-dereference --minimal -Naur linux-6.17-rc4/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5-pro.dts linux-6.17-rc4/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5-pro.dts
