 		};
 	};
 
 	fan: pwm-fan {
 		compatible = "pwm-fan";
-		cooling-levels = <0 95 145 195 255>;
+		cooling-levels = <0 96 128 164 196 224 255>;
 		fan-supply = <&vcc_5v0>;
 		pwms = <&pwm3 0 50000 0>;
