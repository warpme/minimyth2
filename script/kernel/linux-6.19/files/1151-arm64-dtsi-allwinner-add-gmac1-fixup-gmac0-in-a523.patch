diff --speed-large-files --no-dereference --minimal -Naur linux-6.16/arch/arm64/boot/dts/allwinner/sun55i-a523.dtsi linux-6.16/arch/arm64/boot/dts/allwinner/sun55i-a523.dtsi
--- linux-6.16/arch/arm64/boot/dts/allwinner/sun55i-a523.dtsi	2025-07-28 09:32:58.133341369 +0200
+++ linux-6.16/arch/arm64/boot/dts/allwinner/sun55i-a523.dtsi	2025-07-28 09:32:32.497059199 +0200
@@ -183,6 +183,22 @@
 				bias-pull-up;
 			};
 
+			rgmii0_25m_pin: rgmii0-25m-pin {
+				pins = "PH13";
+				allwinner,pinmux = <5>;
+				function = "gmac0";
+				drive-strength = <40>;
+				bias-disable;
+			};
+
+			rgmii1_25m_pin: rgmii1-25m-pin {
+				pins = "PJ10";
+				allwinner,pinmux = <5>;
+				function = "gmac1";
+				drive-strength = <40>;
+				bias-disable;
+			};
+
 			uart0_pb_pins: uart0-pb-pins {
 				pins = "PB9", "PB10";
 				allwinner,pinmux = <2>;
@@ -599,8 +625,8 @@
 			compatible = "allwinner,sun55i-a523-gmac0",
 				     "allwinner,sun50i-a64-emac";
 			reg = <0x04500000 0x10000>;
-			clocks = <&ccu CLK_BUS_EMAC0>;
-			clock-names = "stmmaceth";
+			clocks = <&ccu CLK_BUS_EMAC0>, <&ccu CLK_EMAC0_25M>;
+			clock-names = "stmmaceth", "emac25m";
 			resets = <&ccu RST_BUS_EMAC0>;
 			reset-names = "stmmaceth";
 			interrupts = <GIC_SPI 46 IRQ_TYPE_LEVEL_HIGH>;
--

MiniMyth2

