
# for mainline uboot
# sudo dd if=idbloader.img of=/dev/mmcblk1 seek=64
# sudo dd if=u-boot.itb of=/dev/mmcblk1 seek=16384

part /SPL --source rawcopy --sourceparams="file=@MM_HOME@/images/main/boot/rk3566loaderfiles/idbloader.img" --ondisk mmcblk --no-table --align 32
part /SPL --source rawcopy --sourceparams="file=@MM_HOME@/images/main/boot/rk3566loaderfiles/u-boot.itb"    --ondisk mmcblk --no-table --align 8192

